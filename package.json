{
  "name": "vl-ui-tooltip",
  "version": "0.1.2",
  "description": "Gebruik de tooltip om beschrijvende informatie over een knop, label of eender welk element weer te geven. Wanneer een gebruiker een element aanwijst of aanklikt, wordt de tooltip getoond met de extra informatie.",
  "main": "vl-tooltip.js",
  "scripts": {
    "prepare": "npm run prepare:util",
    "prepare:util": "npm explore vl-ui-util -- npm run util",
    "test": "wct -l chrome,firefox --npm",
    "release": "np",
    "release:prepare": "npm run release:prepare:build && npm run release:prepare:imports && npm run release:prepare:css && npm run release:prepare:commit",
    "release:prepare:build": "uglifyjs --compress --mangle --output vl-tooltip.js -- *.src.js",
    "release:prepare:imports": "replace '/node_modules/vl-ui-core/vl-core.src.js' '/node_modules/vl-ui-core/vl-core.js' vl-tooltip.js",
    "release:prepare:css": "replace '\\.\\./style.css' '/node_modules/vl-ui-tooltip/style.css' vl-tooltip.js",
    "release:prepare:commit": "git add -f vl-tooltip.js && git commit --amend --no-edit && git pull",
    "release:testless": "np --yolo",
    "demo": "http-server"
  },
  "dependencies": {
    "tooltip.js": "1.3.1",
    "vl-ui-core": "1.3.0"
  },
  "devDependencies": {
    "@govflanders/vl-ui-tooltip": "3.7.2",
    "http-server": "0.11.1",
    "np": "3.0.4",
    "replace": "1.0.0",
    "uglify-js-es6": "2.8.9",
    "vl-ui-button": "1.0.0",
    "vl-ui-util": "1.0.3",
    "wct-browser-legacy": "1.0.2",
    "web-component-tester": "6.9.2"
  },
  "peerDependencies": {
    "vl-ui-core": "1.3.0"
  },
  "publishConfig": {
    "registry": "http://artifactory-pr-build.lb.cumuli.be:8081/artifactory/api/npm/acd-npm/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/milieuinfo/webcomponent-vl-ui-tooltip.git"
  },
  "author": "DIDM",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/milieuinfo/webcomponent-vl-ui-tooltip/issues"
  },
  "homepage": "https://github.com/milieuinfo/webcomponent-vl-ui-tooltip#readme"
}
